{
    "version": 2,
    "builds": [{
        "src": "api/simple-test.py",
        "use": "@vercel/python"
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/api/simple-test.py"
        },
        {
            "src": "/",
            "dest": "/test-index.html"
        }
    ]
}